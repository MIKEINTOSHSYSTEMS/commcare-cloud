{
	"Version": "2012-10-17",
	"Statement": [{
			"Effect": "Allow",
			"Action": [
				"s3:ListAllMyBuckets"
			],
			"Resource": "arn:aws:s3:::*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"s3:GetObject",
				"s3:ListBucket",
				"s3:PutObject",
				"s3:PutObjectAcl",
				"s3:RestoreObject"
			],
			"Resource": [
				"arn:aws:s3:::dimagi-commcare-production-logs",
				"arn:aws:s3:::dimagi-commcare-production-logs/*"
			]
		}
	]
}
