resource "aws_route53_zone" "elastic-cache-dns" {
  name = "${var.domain_name}"
  vpc {
    vpc_id = "${var.zone_vpc_id}"
  }
}

resource "aws_route53_record" "elastic-cache-dns-map" {
  zone_id  = "${aws_route53_zone.elastic-cache-dns.zone_id}"
  name     = "${var.route_names}"
  type     = "${var.type}"
  ttl      = "${var.ttl}"
  records  = ["${var.records}"]
}
