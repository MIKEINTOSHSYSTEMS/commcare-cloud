# {{ ansible_managed }}

server {
  listen 127.0.0.1:80;

  location /nginx_stub_status {
      stub_status on;
      access_log off;
      allow 127.0.0.1;
      deny all;
  }
}

server {
  listen {{ ansible_default_ipv4.address }}:80;

  location /nginx-health {
     access_log off;
     return 200 "healthy\n";
  }
}
